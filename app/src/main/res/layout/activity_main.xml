<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.example.giovanne.atmempresaconsultoria.MainActivity">

    <ImageView
        android:id="@+id/atm_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:background="@drawable/logo"
        android:layout_centerHorizontal="true"/>
    <ImageView
        android:id="@+id/img_empresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="62dp"
        android:background="@drawable/menu_empresa"
        android:layout_below="@+id/atm_logo"
        android:layout_alignLeft="@+id/atm_logo"
        android:layout_alignStart="@+id/atm_logo" />
    <ImageView
        android:id="@+id/img_servicos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="62dp"
        android:background="@drawable/menu_servico"
        android:layout_below="@+id/atm_logo"
        android:layout_alignRight="@+id/atm_logo"
        android:layout_alignEnd="@+id/atm_logo" />
    <ImageView
        android:id="@+id/img_cliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="@drawable/menu_cliente"
        android:layout_below="@+id/img_empresa"
        android:layout_alignLeft="@+id/atm_logo"
        android:layout_alignStart="@+id/atm_logo" />
    <ImageView
        android:id="@+id/img_contato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="@drawable/menu_contato"
        android:layout_below="@+id/img_servicos"
        android:layout_alignRight="@+id/atm_logo"
        android:layout_alignEnd="@+id/atm_logo" />


</RelativeLayout>
